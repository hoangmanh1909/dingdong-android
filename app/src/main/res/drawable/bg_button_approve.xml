<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@drawable/bg_confirm_disable"/>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="0"
